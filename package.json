    "express": "^4.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^6.2.0",
    "css-loader": "^6.8.1",
    "nodemon": "^2.0.7",
    "path-browserify": "^1.0.1",
    "serve": "^14.2.4",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}

{
  "name": "json-reader",
  "version": "1.0.0",
  "description": "JSON Reader application with file upload and API request capabilities",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "serve public",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "build": "webpack --mode production",
    "watch": "webpack --mode development --watch"
